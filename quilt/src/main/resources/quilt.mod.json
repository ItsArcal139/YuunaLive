        "com.kakaouo.mods.yuunalive.quilt.YuunaLiveQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.19"
      },
      {
        "id": "architectury",
        "version": ">=5.12.42"
