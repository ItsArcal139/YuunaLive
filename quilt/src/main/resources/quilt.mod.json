{
  "schemaVersion": 1,
  "mixin": [
    "yuunalive.mixins.json",
    "yuunalive-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "yuunalive",
    "version": "${version}",
    "name": "YuunaLive",
    "description": "",
    "authors": [
      "Kaka"
    ],
    "contact": {
      "sources": "https://github.com/KaKusaOAO/YuunaLive"
    },
    "license": "MIT",
    "icon": "assets/yuunalive/icon.png",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "environment": "*",
    "entrypoints": {
